      <!-- Main body joints -->
      <joint name="left_front_joint" type="fixed">
        <parent>base_link</parent>
        <child>left_front_link</child>
      </joint>

      <joint name="right_front_joint" type="fixed">
        <parent>base_link</parent>
        <child>right_front_link</child>
      </joint>

      <joint name="right_back_joint" type="fixed">
        <parent>base_link</parent>
        <child>right_back_link</child>
      </joint>

      <joint name="left_back_joint" type="fixed">
        <parent>base_link</parent>
        <child>left_back_link</child>
      </joint>

      <joint name="keel_connector_joint" type="fixed">
        <parent>base_link</parent>
        <child>keel_connector_link</child>
      </joint>

      <joint name="keel_joint" type="fixed">
        <parent>keel_connector_link</parent>
        <child>keel_link</child>
      </joint>

      <joint name="sail_rotator_joint" type="revolute">
        <axis>
          <xyz>0 0 1</xyz>
            <limit>
            <lower>-1</lower>
            <upper>1</upper>
          </limit>
          <dynamics>
            <damping>1</damping>
          </dynamics>
        </axis>
        
        <parent>base_link</parent>
        <child>sail_rotator_link</child>
      </joint>
      
      <joint name="mast_joint" type="fixed">
        <parent>base_link</parent>
        <child>mast_link</child>
      </joint>

      <joint name="boom_joint" type="fixed">
        <parent>sail_rotator_link</parent>
        <child>boom_link</child>
      </joint>

      <joint name="sail_joint" type="fixed">
        <parent>boom_link</parent>
        <child>sail_link</child>
      </joint>
      

      <!-- Propeller and rudder -->
      <link name='propeller_link'>
        <pose>-.6 0 -.1 0 0 0</pose>
        <inertial>
          <mass>1</mass>
          <inertia>
            <ixx>0.001218375</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.001218375</iyy>
            <iyz>0</iyz>
            <izz>0.001218375</izz>
