              <size>0.0855 0.0855 0.0855</size>
            </box>
          </geometry>
        </visual>
      </link>

      <joint name="propeller_joint" type="revolute">
        <parent>base_link</parent>
        <child>propeller_link</child>
        <axis>
          <xyz>1 0 0</xyz>
          <dynamics>
          </dynamics>
          <limit>
            <lower>-1e+12</lower>
            <upper>1e+12</upper>
          </limit>
        </axis>
      </joint>

      <link name='rudder_link'>
        <pose>-.75 0 -0.15425 0 0 0</pose>

        <inertial>
          <mass>1</mass>
          <inertia>
            <ixx>0.00282940576</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.00401459983</iyy>
            <iyz>0</iyz>
